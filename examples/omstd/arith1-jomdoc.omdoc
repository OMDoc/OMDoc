              <om:OMV name="x"/>
              <om:OMV name="y"/>
            </om:OMBVAR>
            <om:OMA id="abs-prop-1.1">
              <om:OMS name="geq" cd="relation1"/>
              <om:OMA>
                <om:OMS name="plus" cd="arith1"/>
                <om:OMA>
                  <om:OMS name="abs" cd="arith1"/>
                  <om:OMV name="x"/>
                </om:OMA>
                <om:OMA>
                  <om:OMS name="abs" cd="arith1"/>
                  <om:OMV name="y"/>
                </om:OMA>
              </om:OMA>
              <om:OMA>
                <om:OMS name="abs" cd="arith1"/>
                <om:OMA>
                  <om:OMS name="plus" cd="arith1"/>
                  <om:OMV name="x"/>
                  <om:OMV name="y"/>
                </om:OMA>
              </om:OMA>
            </om:OMA>
          </om:OMBIND>
        </om:OMOBJ>
      </FMP>
    </assertion>
    <assertion xml:id="root-prop-1" type="lemma">
