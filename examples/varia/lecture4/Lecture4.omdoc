  <omtext xml:id="C863-Theory4-txt-1">
    <CMP xml:lang="en">
      Classes vs instances
    </CMP>
  </omtext>
  <symbol name="Danny Sleator" scope="global">
    <metadata><dc:subject>Danny Sleator</dc:subject></metadata>
  </symbol>
