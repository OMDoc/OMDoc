      </CMP>
    </axiom>
    <axiom xml:id="C871-Axiom23">
      <metadata>
        <dc:title xml:lang="en">
          abstract classes rule 5
        </dc:title>
        <dc:date action="created">
          20021216
        </dc:date>
        <dc:date action="update">
          20021217
        </dc:date>
        <Language>en</Language>
      </metadata>
      <CMP xml:lang="en">
