<!-- dc:source: <s,t>                   pair  -->
<symbol name="pair" xml:id="nuprl.pair">
<metadata>
<dc:description>The binary pair constructor</dc:description>
</metadata>
</symbol>


<presentation for="#pair" role="applied">
