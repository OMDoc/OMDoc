  <use format="html|pmml" fixity="infix">&#x000D7;</use>
</presentation>

<example xml:id="ex-product" for="#nuprl.product" type="for">
  <CMP>A Cartesian product</CMP>
  <OMOBJ xmlns="http://www.openmath.org/OpenMath">
